<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.mapper.AddressMapper">
<insert id="addAddress" useGeneratedKeys="true" parameterType="com.baizhi.entity.Address" keyProperty="id">
    insert into address(recive_name,province,city,telphone,detail_address,user_id) values (#{reciveName},#{province},#{city},#{telphone},#{detailAddress},#{userId})
</insert>
</mapper>
